    <sep />
    <category name="Advanced" colour="80">
        <block type="wyliolab_analogread">
            <value name="pin">
                <block type="pin_analog"/>
            </value>
        </block>
        <block type="wyliolab_digitalread">
            <value name="pin">
                <block type="pin_digital"/>
            </value>
        </block>
        <block type="wyliolab_analogwrite">
            <value name="pin">
                <block type="pin_analog"/>
            </value>
            <value name="value">
                <block type="math_number"/>
            </value>
        </block>
        <block type="wyliolab_digitalwrite" >
            <value name="pin">
                <block type="pin_digital"/>
            </value>
            <value name="value">
                <block type="logic_boolean"/>
            </value>
        </block>
        <block type="wyliolab_pinmode">
            <value name="NAME">
                <block type="pin_analog"/>
            </value>
        </block>
        <block type="wyliolab_pinmode">
            <value name="NAME">
                <block type="pin_digital"/>
            </value>
        </block>
        <block type="pause" />
    </category>
    <sep />
</toolbox>